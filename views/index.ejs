<h1>Homepage</h1>

<ul>
    <% posts.forEach(post => { %>
        <li>
            <a href="/<%= post.title %>"><%= post.title %></a>
            <a href="/edit/<%= post.title %>">
                <button>Edit</button>
            </a>
        </li>
    <% }) %>
</ul>

<a href="/create">Create New Post</a><br />
